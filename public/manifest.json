{
  "manifest_version": 3,
  "name": "Bryn AI - Proactive Browser Assistant",
  "version": "1.0.0",
  "description": "100% private AI assistant that tracks your research and sends smart nudges",
  "minimum_chrome_version": "138",
  "permissions": [
    "storage",
    "tabs",
    "activeTab",
    "scripting",
    "history",
    "unlimitedStorage",
    "sidePanel",
    "alarms"
  ],
  "host_permissions": ["<all_urls>"],
  "background": {
    "service_worker": "src/background/service-worker.ts",
    "type": "module"
  },
  "side_panel": {
    "default_path": "src/sidepanel/index.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["src/content-scripts/page-tracker.ts"],
      "run_at": "document_idle"
    }
  ],
  "action": {
    "default_title": "Open Bryn AI",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
